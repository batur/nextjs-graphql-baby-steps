{
  "extends": "next/core-web-vitals",
  "rules": {
    "import/no-anonymous-default-export": [
      "warning",
      {
        "allowArray": true,
        "allowArrowFunction": false,
        "allowAnonymousClass": false,
        "allowAnonymousFunction": false,
        "allowCallExpression": true,
        "allowLiteral": true,
        "allowObject": true
      }
    ]
  }
}
